# API URL
REACT_APP_API_URL=http://localhost:5000/api

# WebSocket URL
REACT_APP_SOCKET_URL=http://localhost:5000

# Authentication
REACT_APP_TOKEN_KEY=auth_token
REACT_APP_REFRESH_TOKEN_KEY=refresh_token

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=10485760 # 10MB in bytes
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_VIDEO_CONFERENCE=false
